<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.utkur.yukuzapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/yuk_uz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity_1" />
        <activity android:name=".RegisterActivity_2" />
        <activity android:name=".RegisterActivity_3" />
        <activity android:name=".Term_And_ConditionActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity
            android:name=".BottomActionBarActivity"
            android:label="@string/title_activity_bottom_action_bar"></activity>
    </application>

</manifest>